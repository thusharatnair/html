 <h1>Sign Up</h1>
<div class="container">
<div class="main">
<br>
<form action="#" name="myForm" onsubmit="return(validate());">
<h3 style="color:black;">Create new Account </h3>
<form id="form_id" method="post" name="myform">
<label></label>
<input type="text" placeholder="Username:" name="Username1"  id="username"/><br><br>
<label></label>
<input type="email"  name="email" pattern="/^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/" required   placeholder="Enter Email:" /><br><br>
<label></label>
<div style="float:left; padding-right:2px; width:48%;">
 <label for="Password"></label>
 <input type="password" name="Password" placeholder="Password:" id="pass1"/>
  </div>
  <div style="float:right; padding-left:2px; width:48%;">
  <label for="lname"></label>
 <input  type="text" name="Repeat Password" placeholder="Repeat Password:" id="pass2"/>
 </div><br><br><br>
<input type="checkbox" name="terms" value="terms">I agree to the Terms of Service and Privacy Policy<br><br>
<input type="submit" value="Create Account" id="submit" onclick="javascript:validate()"/>
</form>
<script>
    function myFunction() {
        var pass1 = document.getElementById("pass1").value;
        var pass2 = document.getElementById("pass2").value;
        if (pass1 != pass2) {
            //alert("Passwords Do not match");
            document.getElementById("pass1");
            document.getElementById("pass2");
        }
        else {
            alert("Passwords Match!!!");
        }
    }
</script>
